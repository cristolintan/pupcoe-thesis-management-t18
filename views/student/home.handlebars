<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
  <h1 class="h2">Home</h1>
</div>
<div>
  <h2 class='h3'>Name: {{user.first_name}} {{user.last_name}}</h2>
  <h2 class='h4'>Class: {{user.batch}}-{{user.section}}</h2>
  <h2 class='h4'>Adiviser: {{user.adviser_fname}} {{user.adviser_lname}}</h2>
  <hr>

  <h2 class='h4'>Group: {{group.name}}</h2>

 <h4 class="h4">Group Members</h4><br>
  <table class="table table-sm">
  <thead>
    <tr>
      <th scope="col">Student Number</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col"></th>
    </tr>
  </thead>
  
  <tbody>
    {{#each members}}
    <tr>
      <th scope="row">{{this.student_number}}</th>
      <td>{{this.first_name}} {{this.last_name}}</td>
      <td>{{this.email}}</td>
      <td>{{this.phone}}</td>
      <td><a href='/student/{{this.id}}'>view</a></td>
    </tr>
    {{/each}}
  </tbody>
</table>

  
</div>